function [dPsi_by_dXi] = get_dPsi_by_dXi(n)
%GET_DPSI_BY_DXI Summary of this function goes here
%   Detailed explanation goes here

    switch n
        case 0
            dPsi_by_dXi = -0.5
        case 0
            disp('zero')
        case 1
            disp('positive one')
        otherwise
            disp('other value')
    end
end

